@model IEnumerable<Requestnet.Core.Domain.Casos>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaCreacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Solucion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaSol)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Notas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdClienteNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdEstadoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdJefeNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdLiderNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTecnicoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTipoCasoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaCreacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Solucion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaSol)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Notas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdClienteNavigation.IdCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEstadoNavigation.IdEstado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdJefeNavigation.IdJefe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdLiderNavigation.IdLider)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTecnicoNavigation.IdTecnico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTipoCasoNavigation.IdTipoCaso)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdCaso">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdCaso">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdCaso">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
